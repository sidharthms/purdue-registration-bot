  </entityFramework>
  <system.net>
    <mailSettings>
      <smtp from="registerbot@sidharthms.com">
        <network enableSsl="true" host="smtp.gmail.com" userName="registerbot@sidharthms.com" password="sbot@24648" port="587" />
      </smtp>
    </mailSettings>
  </system.net>
  <system.data>
    <DbProviderFactories>
      <remove invariant="System.Data.SqlServerCe.4.0" />
